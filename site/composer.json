    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4.0",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "autoload": {
